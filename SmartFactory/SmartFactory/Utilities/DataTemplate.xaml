<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:vm="clr-namespace:SmartFactory.ViewModels"
                    xmlns:view="clr-namespace:SmartFactory.Views">

    <DataTemplate DataType="{x:Type vm:HomeVM}">
        <view:Home />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:GraphVM}">
        <view:Graph />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:ManagementVM}">
        <view:Management />
    </DataTemplate>

</ResourceDictionary>